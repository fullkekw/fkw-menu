    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.31",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.0",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.5",
